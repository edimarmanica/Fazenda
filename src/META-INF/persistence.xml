<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="FazendaPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>br.edimarmanica.fazenda.model.domain.TipoCaixa</class>
    <class>br.edimarmanica.fazenda.model.domain.TipoChecklistItemChecklist</class>
    <class>br.edimarmanica.fazenda.model.domain.Pessoa</class>
    <class>br.edimarmanica.fazenda.model.domain.ItemChecklist</class>
    <class>br.edimarmanica.fazenda.model.domain.TipoChecklist</class>
    <class>br.edimarmanica.fazenda.model.domain.Saldo</class>
    <class>br.edimarmanica.fazenda.model.domain.Animal</class>
    <class>br.edimarmanica.fazenda.model.domain.Caixa</class>
    <class>br.edimarmanica.fazenda.model.domain.converters.SexoConverter</class>
    <class>br.edimarmanica.fazenda.model.domain.converters.CorConverter</class>
    <class>br.edimarmanica.fazenda.model.domain.converters.SituacaoAnimalConverter</class>
    <class>br.edimarmanica.fazenda.model.domain.converters.FluxoCaixaConverter</class>
    <class>br.edimarmanica.fazenda.model.domain.converters.BooleanoConverter</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/fazenda"/>
      <property name="javax.persistence.jdbc.user" value="edimar"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="edimar"/>
    </properties>
  </persistence-unit>
</persistence>
